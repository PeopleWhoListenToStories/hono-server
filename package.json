{
  "name": "hono-ai-server",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@9.1.1",
  "description": "Hono GPT API Server",
  "license": "MIT",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "test -f node_modules/.bin/simple-git-hooks && simple-git-hooks || echo 'simple-git-hooks not found, skipping...'",
    "dev": "tsx watch src/index.ts",
    "start": "cross-env NODE_ENV=production node --trace-warnings dist/src/index.js",
    "db:start": "docker compose -f ./docker-compose-db.yaml up -d",
    "db:stop": "docker compose -f ./docker-compose-db.yaml down",
    "db:migrate": "tsx ./cli/db-migrate.ts",
    "db:create": "prisma migrate dev --create-only",
    "db:generate": "prisma generate",
    "db:deploy": "prisma migrate deploy",
    "db:status": "prisma migrate status",
    "test": "cross-env LOG_LEVEL=silent vitest",
    "test:coverage": "vitest --coverage",
    "build": "cross-env NODE_ENV=production tsdown",
    "build:vercel": "tsup src/index.ts --format esm --out-dir api",
    "generate": "drizzle-kit generate --config drizzle.config.ts",
    "push": "drizzle-kit push",
    "studio": "drizzle-kit studio --config drizzle.config.ts",
    "seed": "tsx migrations/seed/index.ts",
    "migrate": "drizzle-kit migrate",
    "lint": "eslint . --ext .ts",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier --write '**/*.ts{,x}'",
    "check:types": "tsc --noEmit",
    "check:spell": "git diff --name-only | cspell --no-summary --no-progress --no-must-find-files --file-list stdin",
    "check:all": "concurrently \"pnpm lint\" \"pnpm check:types\" \"pnpm check:spell\"",
    "pre-commit": "concurrently \"lint-staged --allow-empty\" \"pnpm check:types\" \"pnpm check:spell\""
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^8.1.0",
    "@aws-sdk/client-s3": "^3.925.0",
    "@aws-sdk/s3-request-presigner": "^3.925.0",
    "@cap.js/server": "^4.0.3",
    "@hono/node-server": "1.19.6",
    "@hono/node-ws": "^1.2.0",
    "@hono/sentry": "^1.2.2",
    "@hono/swagger-ui": "^0.4.1",
    "@hono/zod-openapi": "^1.1.4",
    "@neondatabase/serverless": "^1.0.2",
    "@node-rs/argon2": "^2.0.2",
    "@scalar/hono-api-reference": "^0.9.24",
    "bullmq": "^5.63.0",
    "casbin": "^5.40.0",
    "croner": "^9.1.0",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dayjs": "^1.11.13",
    "dotenv": "^17.2.3",
    "dotenv-expand": "^12.0.3",
    "drizzle-orm": "^0.44.7",
    "drizzle-zod": "^0.8.3",
    "gpt-3-encoder": "^1.1.4",
    "gpt-tokenizer": "^3.3.0",
    "hono": "^4.10.4",
    "hono-pino": "^0.10.3",
    "hono-rate-limiter": "^0.4.2",
    "http-status-codes": "^2.3.0",
    "iconv-lite": "^0.7.0",
    "ioredis": "5.8.2",
    "kysely": "^0.27.4",
    "lodash": "^4.17.21",
    "openai": "^6.8.1",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2",
    "postgres": "^3.4.7",
    "prisma-kysely": "^1.8.0",
    "rate-limit-redis": "^4.2.3",
    "type-fest": "^5.2.0",
    "uuid": "^13.0.0",
    "yjs": "^13.6.27",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.2.0",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@faker-js/faker": "^9.2.0",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "24.10.0",
    "@types/pg": "^8.15.6",
    "@types/swagger-jsdoc": "^6.0.4",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "@vitest/coverage-v8": "^2.1.9",
    "chalk": "^5.3.0",
    "concurrently": "^9.1.0",
    "cross-env": "^10.1.0",
    "cspell": "^8.16.0",
    "drizzle-kit": "^0.31.6",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-format": "^1.0.2",
    "husky": "^9.1.6",
    "lint-staged": "^16.2.6",
    "prettier": "^3.3.3",
    "prisma": "^5.22.0",
    "simple-git-hooks": "^2.13.1",
    "swagger-jsdoc": "^6.2.8",
    "tsc-alias": "^1.8.10",
    "tsdown": "^0.16.0",
    "tsup": "^8.3.5",
    "tsx": "^4.20.6",
    "typescript": "5.9.3",
    "vite-tsconfig-paths": "^5.1.1",
    "vitest": "^4.0.7"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
